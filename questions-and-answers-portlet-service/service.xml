<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="de.sidate.questions_and_answers">

	<namespace>SIDATE</namespace>

    <entity name="Content" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="contentID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <column name="text" type="String" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

	<entity name="Answer" uuid="true" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="contentID" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />


		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
	</entity>

    <entity name="Comment" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="contentID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <column name="targetID" type="long" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

    <entity name="Question" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="contentID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <column name="title" type="String" />
        <column name="categories" type="Collection" />
        <column name="tags" type="Collection" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

    <entity name="Rating" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="ratingID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <column name="isUpvote" type="Boolean" />
        <column name="contentID" type="long" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

    <entity name="Reputation" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="reputationID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

    <entity name="Category" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="categoryID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <column name="name" type="String" />
        <column name="color" type="String" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

    <entity name="Tag" uuid="true" local-service="true" remote-service="true">

        <!-- PK fields -->

        <column name="tagID" type="long" primary="true" />

        <!-- Group instance -->

        <column name="groupId" type="long" />

        <!-- Audit fields -->

        <column name="companyId" type="long" />
        <column name="userId" type="long" />
        <column name="userName" type="String" />
        <column name="createDate" type="Date" />
        <column name="modifiedDate" type="Date" />

        <column name="name" type="String" />

        <!-- Finder methods -->

        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId" />
        </finder>
    </entity>

</service-builder>